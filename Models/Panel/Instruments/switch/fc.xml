<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>on-offR.ac</path>
  <animation>
    <!-- Objets opaques -->
    <object-name>base</object-name>
    <object-name>on-off</object-name>
    
    <!-- Objets transparents -->
  </animation>
  
<effect>
<inherits-from>Aircraft/EchoAirXray/Models/Effects/switch-lightmap</inherits-from>
<object-name>on-off</object-name>
</effect>
  
  <animation>
    <object-name>on-off</object-name>
    <type>rotate</type>
    <property>controls/electric/fuel-cell</property>
    <factor>  35 </factor>
    <min-deg>  0 </min-deg>
    <max-deg> 35 </max-deg>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>pick</type>
    <object-name>on-off</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
    <property>controls/electric/fuel-cell</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/sound/click</property>
      </binding><!--
      <binding>
	<command>property-toggle</command>
        <property>/sim/model/material/instruments/factor</property>
      </binding>-->
    </action>
  </animation>
  
</PropertyList>
